{
  "name": "webmonster-community/php-helpers",
  "description": "An extensive set of PHP helper functions",
  "keywords": [
    "php",
    "helpers",
    "class"
  ],
  "homepage": "https://webmonster.tech/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "webmonster",
      "email": "hello@webmonster.tech",
      "homepage": "https://github.com/Webmonster-Community/HeadGeneratorPhp/contributors"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-intl": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "rector/rector": "^0.15.23",
    "phpstan/phpstan": "^1.3.3",
    "phpstan/phpdoc-parser": "^1.2",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "squizlabs/php_codesniffer": "^3.7",
    "friendsofphp/php-cs-fixer": "^3.16"
  }
}
